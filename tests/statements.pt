int i = 4
for(int i : [3, 2, 1]) {
    print(i++" ")
}
print(i++"\n")

for(int i: 0..4) {
    print(i++" ")
}
print("\n");

for(int i: 5..1) {
    print(i++" ")
}
print("\n");

for(int i: 10, 7..0) {
    print(i++" ")
}
print("\n");

for(string i: "abc") {
    print(i++" ")
}
print("\n");

for(int i: -4,-2..1) {
    print(i++" ")
}
print("\n");

for(int i: 0..length("hi there\n")) {
    print("hi there\n"[i])
}

int a = 0
while (a < 10) {
    a += 1
}
print(a ++ "\n");

a = 0
do {
    a += 1
} while (a < 10)
print(a ++ "\n");