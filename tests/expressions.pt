bool none_test(int? x) {
    return x == none
}

string matrix_test(int[] x) {
    string y
    y ++= "(" ++ x[0] ++ " - " ++ x[length(x) - 1] ++ ")"
    for(int i : x) {
        y ++= ", "++i
    }
    return y ++ "\n"
}

